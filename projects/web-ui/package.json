{
  "name": "web-ui",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci:checks": "npm ci && npm run blueprint2swagger && npm run lint && npm run test:ci && npm run e2e",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run blueprint2swagger && npm run build:browser:serverless && npm run build:server:serverless && npm run package",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "package": "serverless package",
    "build:server:prod": "webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "webpack --config webpack.server.config.js --progress --colors",
    "blueprint2swagger": "apib2swagger -i blueprints/*.md -o blueprints/swagger.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.4",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ng-toolkit/serverless": "^1.1.51",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "aws-serverless-express": "^3.2.0",
    "chart.js": "^2.7.3",
    "color": "^3.1.0",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "leaflet": "^1.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngrx/schematics": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@types/chart.js": "^2.7.48",
    "@types/color": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.4.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "~8.9.4",
    "apib2swagger": "^1.8.0",
    "codelyzer": "~4.5.0",
    "dredd-transactions": "^6.5.2",
    "express": "^4.16.4",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "opencollective": "^1.0.3",
    "protractor": "~5.4.0",
    "serverless": "1.26.1",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.1.2"
  }
}
